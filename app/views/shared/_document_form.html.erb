<%= form_for(@document) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
  	<%= f.label :date, "日付" %>
	<%= f.text_field :date %>

	<%= f.label :weather, "天気" %>
	<%= f.text_field :weather %>

  	<%= f.label :content, "本文" %>
	<%= f.text_area :content %>

	<%= f.label :lat, "緯度" %>
	<%= f.text_field :lat %>

	<%= f.label :lng, "経度" %>
	<%= f.text_field :lng %>

  </div>
  <%= f.submit "投稿", class: "ui-btn" %>
<% end %>